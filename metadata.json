{
  "name": "vim_puppet",
  "version": "1.0.0",
  "author": "prabin5",
  "summary": "Setup vim plugins and packages for puppet development",
  "license": "Apache-2.0",
  "source": "https://github.com/prabin5/vim_puppet.git",
  "issues_url": "https://github.com/waymilky/vim_puppet/issues",
  "project_page": "https://github.com/waymilky/vim_puppet",
  "dependencies": [
     { "name": "puppetlabs-stdlib", "version_requirement": ">= 1.0.0 < 5.0.0" },
     { "name": "puppetlabs-vcsrepo", "version_requirement": ">= 1.3.1 < 2.0.0"}
   ],
  "operatingsystem_support": [
     {
       "operatingsystem": "RedHat",
       "operatingsystemrelease": [
         "5",
         "6",
         "7"
        ]
      },
      {
        "operatingsystem": "CentOS",
        "operatingsystemrelease": [
          "5",
          "6",
          "7"
         ]
      },
      {
         "operatingsystem": "Ubuntu",
         "operatingsystemrelease": [
           "10.04",
           "12.04",
           "14.04"
          ]
      }
  ]
}
